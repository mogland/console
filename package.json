  "scripts": {
    "dev": "vite --port 3000",
    "build": "tsc && vite build",
    "build:mog": "tsc && BUILD_MODE=BUILT_IN vite build",
    "preview": "vite preview",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.tsx\"",
