    "@types/node": "18.15.8",
    "@types/react": "18.0.29",
    "@types/react-dom": "18.0.11",
    "@vitejs/plugin-react": "3.1.0",
    "autoprefixer": "10.4.14",
    "clsx": "^1.2.1",
    "less": "4.1.3",
    "postcss": "8.4.21",
    "postcss-import": "15.1.0",
    "prettier": "2.8.7",
    "tailwindcss": "3.2.7",
    "typescript": "5.0.2",
    "vite": "4.2.1"
