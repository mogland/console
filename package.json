    "prepublishOnly": "npm run build:mog",
    "publish-release": "pnpm publish --access public",
    "publish-next": "pnpm publish --access public --tag next"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@headlessui/react": "^1.7.13",
    "@icon-park/react": "^1.4.2",
