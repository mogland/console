    "md5": "2.3.0",
    "ofetch": "^1.0.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-dialog": "^1.2.1",
    "react-message-popup": "1.1.1",
    "react-router-dom": "6.9.0",
    "react-use": "17.4.0",
    "sonner": "^0.3.0",
    "swr": "^2.1.3",
    "valtio": "^1.10.3",
    "vditor": "^3.9.1"
